{
    "name": "outsanity/ip-analysis-helper",
    "description": "Tool for converting IANA CSV files to PHP data files",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.23",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0.2",
        "outsanity/ip-analysis": "dev-main",
        "phpdocumentor/reflection-docblock": "^5.6.4",
        "symfony/console": "^6.4.27",
        "symfony/dotenv": "^6.4.24",
        "symfony/flex": "^2.10.0",
        "symfony/framework-bundle": "^6.4.27",
        "symfony/property-access": "^6.4.25",
        "symfony/property-info": "^6.4.24",
        "symfony/runtime": "^6.4.26",
        "symfony/serializer": "^6.4.27",
        "symfony/yaml": "^6.4.26"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^2.1.32",
        "rector/rector": "^2.2.8",
        "squizlabs/php_codesniffer": "^3.13.5",
        "symfony/maker-bundle": "^1.64"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "audit": {
            "abandoned": "fail"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dharple/ip-analysis"
        }
    ]
}
