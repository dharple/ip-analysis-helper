{
    "name": "outsanity/ip-analysis-helper",
    "description": "Tool for converting IANA CSV files to PHP data files",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.13.3",
        "doctrine/orm": "^2.13.3",
        "outsanity/ip-analysis": "dev-master",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/console": "^5.4.15",
        "symfony/dotenv": "^5.4.5",
        "symfony/flex": "^1.19.3",
        "symfony/framework-bundle": "^5.4.14",
        "symfony/property-access": "^5.4.15",
        "symfony/property-info": "^5.4.15",
        "symfony/serializer": "^5.4.15",
        "symfony/yaml": "^5.4.14"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^1.9.2",
        "phpunit/php-code-coverage": "^7.0.15",
        "squizlabs/php_codesniffer": "^3.7.1",
        "symfony/maker-bundle": "^1.47"
    },
    "config": {
        "platform": {
            "php": "8.1.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dharple/ip-analysis"
        }
    ]
}
