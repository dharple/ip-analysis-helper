{
    "name": "outsanity/ip-analysis-helper",
    "description": "Tool for converting IANA CSV files to PHP data files",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Doug Harple",
            "email": "dharple@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.14.3",
        "doctrine/orm": "^2.19.7",
        "outsanity/ip-analysis": "dev-main",
        "phpdocumentor/reflection-docblock": "^5.4.1",
        "symfony/console": "^6.4.11",
        "symfony/dotenv": "^6.4.10",
        "symfony/flex": "^2.4.6",
        "symfony/framework-bundle": "^6.4.10",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^6.4.11",
        "symfony/property-info": "^6.4.10",
        "symfony/runtime": "^6.4",
        "symfony/serializer": "^6.4.11",
        "symfony/yaml": "^6.4.11"
    },
    "require-dev": {
        "outsanity/phpcs": ">=0.4.0",
        "phpstan/phpstan": "^1.12.0",
        "rector/rector": "^0.18.13",
        "squizlabs/php_codesniffer": "^3.10.2",
        "symfony/maker-bundle": "^1.61"
    },
    "config": {
        "platform": {
            "php": "8.1.2"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "audit": {
            "abandoned": "fail"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "phpcbf":  "phpcbf",
        "phpcs":   "phpcs",
        "phpstan": "phpstan analyse",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dharple/ip-analysis"
        }
    ]
}
